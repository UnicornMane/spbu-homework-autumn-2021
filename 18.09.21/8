#define _USE_MATH_DEFINES

#include <iostream>
#include <fstream>
#include <iomanip>
#include <utility>
#include <cmath>
#include <vector>
#include <algorithm>
#include <stack>


using namespace std;

const int INF = 2e9;
const int mod = 1e9 + 7;

struct Time
{
    int sum;

    void in()
    {
        int h, m, s;
        cin >> h >> m >> s;
        sum = 3600 * h + 60 * m + s;
    }
};

signed main()
{
    Time tb, tn;
    tb.in();
    tn.in();

    cout << tn.sum - tb.sum;
}
